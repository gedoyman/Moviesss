<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.littleaozora.hendra.moviessss">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.littleaozora.hendra.moviessss.Moviesss"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.littleaozora.hendra.moviessss.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.littleaozora.hendra.moviessss.DetailActivity"

            android:parentActivityName="com.littleaozora.hendra.moviessss.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.littleaozora.hendra.moviessss.MainActivity" />
        </activity>
    </application>

</manifest>